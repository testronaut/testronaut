{
  "name": "angular-wide",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "tests/angular-wide/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tests/angular-wide",
        "index": "tests/angular-wide/src/index.html",
        "browser": "tests/angular-wide/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "tests/angular-wide/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "tests/angular-wide/public"
          }
        ],
        "scripts": []
      },
      "configurations": {
        "ct": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "browser": "tests/angular-wide/ct-tests/main.ts",
          "index": "tests/angular-wide/ct-tests/index.html",
          "tsConfig": "tests/angular-wide/ct-tests/tsconfig.json"
        }
      },
      "defaultConfiguration": "ct"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "ct": {
          "buildTarget": "angular-wide:build:ct"
        }
      },
      "defaultConfiguration": "ct"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "angular-wide:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "angular-wide:build",
        "port": 4200,
        "staticFilePath": "dist/tests/angular-wide/browser",
        "spa": true
      }
    }
  }
}
